var num = (array)=>{
    var data = [];
    array.forEach(element => {
        var count = 0;
        for(i=2;i<element;i++){
            if(element%i ==0){
                count+=1
            }
         }
        if(count==0){
            data.push(element)
        }
    });
    return data
}
console.log(num([4,5,6,7]));


