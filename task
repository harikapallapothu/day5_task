var string = (array)=>{
    var data = [];
    for(i=0;i<array.length;i++){
        var str = array[i].split('');
        var rev = str.reverse();
        var revstr = rev.join("");
        if(array[i]==revstr){
            data.push(array[i])
        }
    }
    return data
}
console.log(string(["apple",'bob']));
console.log(string(['11', '12', '121']))
      
